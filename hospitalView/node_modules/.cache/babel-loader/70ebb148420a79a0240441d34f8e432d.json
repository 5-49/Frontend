ue: 'navigation',\r\n            label: '内分泌科',\r\n          }, {\r\n            value: 'others',\r\n            label: '心血管内科'\r\n          }]\r\n        },\r\n        {\r\n          value: 'ziyuan',\r\n          label: '五官科',\r\n          children: [{\r\n            value: 'axure',\r\n            label: '耳鼻喉科'\r\n          }, {\r\n            value: 'sketch',\r\n            label: '眼科'\r\n          }, {\r\n            value: 'jiaohu',\r\n            label: '口腔科'\r\n          }]\r\n        }],\r\n      };\r\n    },\r\n\r\n     created () {\r\n    this.getRoomList()\r\n  },  \r\n\r\n    methods:{\r\n      async getRoomList () {\r\n      const { data: res } = await this.$http.get('/room')\r\n      console.log(res.data)\r\n      this.roomList=res.data\r\n    },\r\n        reset(){\r\n            //console.log(this);\r\n            this.$refs.search14Ref.resetFields();\r\n            return this.$message.success('你成功重置了信息！');\r\n        },\r\n        search(){\r\n            //this.$router.push(\"/home\");\r\n        },\r\n        goBack() {\r\n        this.$router.push(\"/home\");\r\n      }\r\n    }\r\n  };\r\n</script>"],"sourceRoot":"src/components/living"}]}